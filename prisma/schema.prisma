generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model SimulationScenario {
  id              String   @id @default(cuid())
  createdAt       DateTime @default(now())
  name            String
  baseAsset       String
  initialCapital  Float
  riskTolerance   String
  notes           String?
}
